\%.*: comment
\b[\w\d]*\b(?=\s?\(.*\)): function
\bif(?=.*)|if\selse(?=.*)|else\b(?=.*): ifelifelse
(\bfor\b): for-loops
(\bwhile\b)(?=\s.+): while-loops
\n[\s\t]*\bend\b: loop-end
\btrue\b|\bfalse\b: booliean
\'.*?\': string
(?<=\.)[\w\d]+: structure
\breturn\b|\bbreak\b: specialstatements