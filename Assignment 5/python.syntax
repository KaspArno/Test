\#.*: comment
\b[\w\d]*\b(?=\s?\(): function
(?<=\()[\w\d]*(?=\)): argument
\bif|elif|else\b(?=.*:): ifelifelse
(\bfor\b)(?=\s.*in.*:): for-loops
(\bwhile\b)(?=\s.+:): while-loops
(?<=import)\s+[\w\d]+: imports
\bimport\b(?=.+): import
\bfrom\b(?=.+\bimport\b): fromimport
\bdef\b(?=\s+[\w\d]+\s?\(.*\)\s*:): def
\bTrue\b|\bFalse\b|\bNone\b: specialstatements
\".*?\": string